{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "migration:create": "cd orm/migrations && typeorm migration:create",
    "migrate": "NODE_PATH=./ ts-node ./node_modules/typeorm/cli.js migration:run -d ./orm/DbCreateConnection",
    "start": "nodemon --exec ts-node --ext ts ./bin/www",
    "dev": "debug=* ts-node-dev --respawn ./bin/index.ts",
    "prod": "NODE_PATH=./dist node ./dist/bin/www.js",
    "build": "rimraf dist && tsc && cp -r views/ dist/",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint ./**/*.ts --fix"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.451.0",
    "@aws-sdk/client-ses": "^3.451.0",
    "@aws-sdk/client-sqs": "^3.451.0",
    "@aws-sdk/client-ssm": "^3.451.0",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-errors": "~2.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.16",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/swagger-jsdoc": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0"
  }
}
